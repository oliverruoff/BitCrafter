-- animation counter 0>ac<30
anim_cnt = 0

function _init()
	gen_floor(0)
	-- music(0)
end

function _update()
 -- animation counter reset
	if (anim_cnt==30)	anim_cnt=0
	
	-- player movement
 if (btn(‚¨ÖÔ∏è)) then
 	plr_move(‚¨ÖÔ∏è)
	elseif (btn(‚û°Ô∏è)) then 
		plr_move(‚û°Ô∏è)
	elseif (btn(‚¨ÜÔ∏è)) then 
		plr_move(‚¨ÜÔ∏è)
	elseif (btn(‚¨áÔ∏è)) then 
		plr_move(‚¨áÔ∏è)
	elseif (not btn(‚¨ÜÔ∏è) 
	    and not btn(‚¨áÔ∏è)
	    and not btn(‚¨ÖÔ∏è)
	    and not btn(‚û°Ô∏è)) then
	 plr_stand()
	end
	
	-- a pressed
	if (btn(‚ùé)) then
	 -- log.txt = "‚ùé"
	 destroy_block()
	elseif (not btn(x)) then
	 restore_block()
	end
	
	-- b pressed
	if (btn(üÖæÔ∏è)) then
	 -- log.txt = "üÖæÔ∏è"
	end
	
	anim_cnt += 1
end

