-- animation counter 0>ac<30
anim_cnt = 0

-- flag if bag should be drawn
show_bag = false

function _init()
	gen_floor(0)
	music(0)
end

function _update()
 -- animation counter reset
	if (anim_cnt==30)	anim_cnt=0
	
	-- if bag should be shown
	-- if so, stop everything else 
	if (btn(‚ùé) and btn(üÖæÔ∏è)) then
	 show_bag  = true
	 return
	else
	 show_bag = false
	end
	
	-- player movement
 if (btn(‚¨ÖÔ∏è)) then
 	plr_move(‚¨ÖÔ∏è)
	elseif (btn(‚û°Ô∏è)) then 
		plr_move(‚û°Ô∏è)
	elseif (btn(‚¨ÜÔ∏è)) then 
		plr_move(‚¨ÜÔ∏è)
	elseif (btn(‚¨áÔ∏è)) then 
		plr_move(‚¨áÔ∏è)
	elseif (not btn(‚¨ÜÔ∏è) 
	    and not btn(‚¨áÔ∏è)
	    and not btn(‚¨ÖÔ∏è)
	    and not btn(‚û°Ô∏è)) then
	 plr_stand()
	end
	-- updating plr target
 plr.tarx, plr.tary =
  plr_tar_xy()
	-- a pressed
	if (btn(üÖæÔ∏è)) then
	 -- log.txt = "‚ùé"
	 destroy_block()
	end
	
	-- b pressed
	if (btn(‚ùé)) then
	 -- log.txt = "üÖæÔ∏è"
	end
	anim_cnt += 1
end

function _draw()
 cls()
 -- draw the map
 --map(0,0)
 draw_map()
 -- draw the player
 draw_plr()
 -- draw the target
 draw_tar()
 draw_ui()
 if (show_bag) then
  draw_bag()
 end
 draw_log()
end
