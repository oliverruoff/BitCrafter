-- map

-- map grid holding
-- all visited floors
mapg = {}

-- different blocks
-- s = sprite <int>
-- w = walkable <bool>
grass  =  {["s"]=33,["w"]=true}
border =  {["s"]=34,["w"]=false}
tree   =  {["s"]=35,["w"]=false}
stone  =  {["s"]=37,["w"]=false}



--random int between l,h
function randb(l,h) --inclusive
    return flr(rnd(h+1-l))+l
end

function gen_floor(z)
 local grid = {}
 for x = 0, 16 do
  grid[x] = {}
  for y = 0, 16 do
   local rand = randb(1,100)
   local block=0
   if rand > 80 and
          rand < 90 then
    block=tree
   elseif rand > 90 and
          rand < 95 then 
    block=stone
   else
    block=grass
   end
    -- setting borders
    if x == 0  or y == 0 or 
       x == 15 or y == 15 then
     block = border
    end 
    -- finally saving blocks
    grid[x][y] = block
  end
 end
 mapg[z] = grid
end

function draw_map()
 local floor=mapg[plr_z]
 for x=0, 16 do
  for y=0, 16 do
   -- actual drawing
	  spr(floor[x][y].s, x*8, y*8)
  end
 end

end

