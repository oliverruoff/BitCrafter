-- returning x,y of tar
function plr_tar_xy()
 local tar_x = flr(plr.x/8)
 local tar_y = flr(plr.y/8)
 if (plr.hdng == ‚¨ÜÔ∏è) then
  tar_y -= 1
 elseif (plr.hdng == ‚¨áÔ∏è) then
  tar_y += 1
 elseif (plr.hdng == ‚¨ÖÔ∏è) then
  tar_x -= 1
 elseif (plr.hdng == ‚û°Ô∏è) then
  tar_x += 1
 end
 plr.tar = mapg[plr.z]
              [tar_x]
              [tar_y]
 return tar_x, tar_y
end

-- drawing target of player
function draw_tar()
 -- drawing rectangle
 spr(16, -- tar sprite
     plr.tarx*8,
     plr.tary*8)
end

-- only called when btn is not
-- pressed
function destroy_block()
 if (plr.tar.d ~= -1) then
  plr.tar.r -= 1
  -- block destroyed
  if (plr.tar.r <= 0) then
   -- adding loot to plr
   plr.bag[plr.tar.l] += 1
   -- adding new copy of grass
   mapg[plr.z]
       [plr.tarx]
       [plr.tary] = t_copy(
                  b_map[0])
   -- setting new plr tar
   plr.tar = mapg[plr.z]
       [plr.tarx]
       [plr.tary]
   -- starting destruction anim 
	  mapg[plr.z]
	      [plr.tarx]
	      [plr.tary].e =
	       destroy_anim_start_spr
  end   
 end
end

