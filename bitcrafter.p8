--random int between l,h
function randb(l,h) --inclusive
 return flr(rnd(h+1-l))+l
end

function gen_floor(z)
 local grid = {}
 for x = 0, 16 do
  grid[x] = {}
  for y = 0, 16 do
   local rand = randb(1,100)
   local block=0
   if rand > 80 and
          rand < 90 then
    block=b_map[2]
   elseif rand > 90 and
          rand < 95 then 
    block=b_map[3]
   else
    block=b_map[0]
   end
    -- setting borders
    if x == 0  or y == 0 or 
       x == 15 or y == 15 then
     block = b_map[1]
    end 
    -- grass block under plr
    if x==7 and y==7 then
     block = b_map[0]
    end
    -- finally saving blocks
    grid[x][y] = t_copy(block)
  end
 end
 mapg[z] = grid
end

function draw_map()
 local floor=mapg[plr.z]
 for x=0, 16 do
  for y=0, 16 do
	  spr(floor[x][y].s, x*8, y*8)
  end
 end

end

