 -- moving player up
	if (dir==‚¨ÜÔ∏è) then
		if (mapg[plr.z]
		        [flr(plr.x/8)]
		        [flr((plr.y-1)/8)].w)
		 then
		  plr.y -= 1
		end
		plr.hdng = ‚¨ÜÔ∏è
		if anim_cnt%5==0 then
		 if (plr.spr==17) then
		  plr.spr = 18
		 elseif plr.spr==18 then
		  plr.spr = 17
		 else
		  plr.spr = 17
		 end
		end
	-- moving player down
	elseif (dir==‚¨áÔ∏è) then
	 if (mapg[plr.z]
		        [flr(plr.x/8)]
		        [flr((plr.y+1)/8)].w)
		then
		 plr.y += 1
		end
