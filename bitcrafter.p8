-- animation counter 0>ac<30
anim_cnt = 0

function _init()
	gen_floor(0)
end

function _update()
 -- animation counter reset
	if (anim_cnt==30)	anim_cnt=0
 if (btn(⬅️)) then
 	plr_move(⬅️)
	elseif (btn(➡️)) then 
		plr_move(➡️)
	elseif (btn(⬆️)) then 
		plr_move(⬆️)
	elseif (btn(⬇️)) then 
		plr_move(⬇️)
	elseif (    not btn(⬆️) 
	    and not btn(⬇️)
	    and not btn(⬅️)
	    and not btn(➡️)) then
	 plr_stand()
	end
	anim_cnt += 1
end

function _draw()
 cls()
 -- draw the map
 --map(0,0)
 draw_map()
 -- draw the player
 draw_plr()
 draw_ui()
 draw_log()
end
