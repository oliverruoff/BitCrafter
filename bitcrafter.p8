-- player

-- player spr to be drawn
plr_spr = 1
-- player coordinates
plr_x = 60
plr_y = 60
plr_z = 0
-- player stats
plr_h_max = 10
plr_h_cur = 10
-- indicates player stand still
plr_standing = true
-- indicates facing direction
plr_hdng = ⬆️ -- ⬆️/⬇️/⬅️/➡️

function draw_plr()
 if (plr_standing) then
		if (anim_cnt < 15) then
		 if (plr_hdng==⬆️) then
		  plr_spr = 1
		 elseif (plr_hdng==⬇️) then
		  plr_spr = 3
		 elseif (plr_hdng==⬅️) then
		  plr_spr = 5
		 elseif (plr_hdng==➡️) then
		  plr_spr = 7
		 end
	 else
	 	if (plr_hdng==⬆️) then
		  plr_spr = 2
		 elseif (plr_hdng==⬇️) then
		  plr_spr = 4
		 elseif (plr_hdng==⬅️) then
		  plr_spr = 6
		 elseif (plr_hdng==➡️) then
		  plr_spr = 8
		 end 
	 end
	end
	-- actual drawing
	spr(plr_spr, plr_x-4, plr_y-4)
end

function plr_move(dir)
 plr_standing = false
 -- moving player up
	if (dir==⬆️) then
		if (mapg[plr_z]
		        [flr(plr_x/8)]
		        [flr((plr_y-1)/8)].w)
		 then
		  plr_y -= 1
		end
		plr_hdng = ⬆️
		if anim_cnt%5==0 then
		 if (plr_spr==17) then
		  plr_spr = 18
		 elseif plr_spr==18 then
		  plr_spr = 17
		 else
		  plr_spr = 17
		 end
		end
	-- moving player down
	elseif (dir==⬇️) then
	 if (mapg[plr_z]
		        [flr(plr_x/8)]
		        [flr((plr_y+1)/8)].w)
		then
		 plr_y += 1
		end
	 plr_hdng = ⬇️
		if anim_cnt%5==0 then
		 if (plr_spr==19) then
		  plr_spr = 20
		 elseif plr_spr==20 then
		  plr_spr = 19
		 else
		  plr_spr = 19
		 end
		end
	-- moving player left
	elseif (dir==⬅️) then
	 plr_hdng = ⬅️
	 		if (mapg[plr_z]
		        [flr((plr_x-1)/8)]
		        [flr(plr_y/8)].w)
		 then
		  plr_x -= 1
		end
		if anim_cnt%5==0 then
		 if (plr_spr==21) then
		  plr_spr = 22
		 elseif plr_spr==22 then
		  plr_spr = 21
		 else
		  plr_spr = 21
		 end
		end
	-- moving player right
	elseif (dir==➡️) then
	 plr_hdng = ➡️
	 		if (mapg[plr_z]
		        [flr((plr_x+1)/8)]
		        [flr(plr_y/8)].w)
		 then
		  plr_x += 1
		end
			if anim_cnt%5==0 then
		 	if (plr_spr==23) then
		   plr_spr = 24
		  elseif plr_spr==24 then
		   plr_spr = 23
		  else
		   plr_spr = 23
		 end
		end
	end	
end


-- when no move btn pressed
function plr_stand()
	plr_standing = true
end
