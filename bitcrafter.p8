destroy_anim_end_spr = 53

--random int between l,h
function randb(l,h) --inclusive
 return flr(rnd(h+1-l))+l
end

function gen_floor(z)
 local grid = {}
 for x = 0, 16 do
  grid[x] = {}
  for y = 0, 16 do
   local rand = randb(1,100)
   local block=0
   -- 10% tree chance
   if rand > 80 and
          rand < 90 then
    block=b_map[2]
   -- 5% rock chance
   elseif rand > 90 and
          rand < 95 then 
    block=b_map[3]
   else
    -- setting grass evrwhrelse
    rand2 = randb(1,100)
    -- 10% chance of animated
    -- grass
    if (rand2 > 90) then
     block=b_map[0]
    else
     -- rest unanimated grass
     block=b_map[4]
    end
   end
    -- setting borders
    if x == 0  or y == 0 or 
       x == 15 or y == 15 then
     block = b_map[1]
    end 
    -- grass block under plr
    if x==7 and y==7 then
     block = b_map[0]
    end
    -- finally saving blocks
    grid[x][y] = t_copy(block)
  end
 end
 mapg[z] = grid
end

function draw_map()
 local floor=mapg[plr.z]
 for x=0, 16 do
  for y=0, 16 do
   -- animating sprite
   if (floor[x][y].s < 
	   floor[x][y].ae) then
	   if (anim_cnt%
	       b_anim_sp == 0) then
 	   floor[x][y].s += 1
			 end
	  else
		  if (anim_cnt%
		      b_anim_sp == 0) then
	 	   floor[x][y].s =
	    floor[x][y].as
				end
	  end
	  -- draw block sprite
	  spr(floor[x][y].s, x*8, y*8)
	  -- check if block is damaged
	  if (floor[x][y].r < 
	      floor[x][y].d) then
	   local b_hp_len = flr( 
	    (floor[x][y].r/
	     floor[x][y].d)*8)
	   -- draw block hp line
	   line(
	    x*8,
	    y*8+1,
	    x*8+b_hp_len,
	    y*8+1,
	    10)
	  end
	  -- check if block should be
	  -- restored because plr
	  -- lost focus before destroy
   if (x ~= plr.tarx or 
       y ~= plr.tary) then
    floor[x][y].r =
     floor[x][y].d   
   end	  
	  -- check if block destroyed
	  -- to animate explosion
	  if (floor[x][y].e > 0
	   and floor[x][y].e <= 
	   destroy_anim_end_spr) then
	   spr(floor[x][y].e, x*8, y*8)
	   if (anim_cnt%3 == 0) then
 	   floor[x][y].e += 1
			 end
	  end
  end
 end

end

