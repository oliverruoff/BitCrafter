-- when no move btn pressed
function plr_stand()
	plr.stand = true
end

-- returning x,y of tar
function plr_tar_xy()
 local tar_x = flr(plr.x/8)
 local tar_y = flr(plr.y/8)
 if (plr.hdng == ‚¨ÜÔ∏è) then
  tar_y -= 1
 elseif (plr.hdng == ‚¨áÔ∏è) then
  tar_y += 1
 elseif (plr.hdng == ‚¨ÖÔ∏è) then
  tar_x -= 1
 elseif (plr.hdng == ‚û°Ô∏è) then
  tar_x += 1
 end
 plr.tar = mapg[plr.z]
              [tar_x]
              [tar_y]
 return tar_x, tar_y
end

-- drawing target of player
function draw_tar()
 local tar_x, tar_y = plr_tar_xy()
 -- drawing rectangle
 rect(tar_x*8,
      tar_y*8,
      tar_x*8+8,
      tar_y*8+8,
      8)
end

function destroy_block()
 if (plr.tar.d ~= -1) then
  plr.tar.r -= 1
  if (plr.tar.r <= 0) then
   local tar_x, tar_y =
    plr_tar_xy()
   -- adding new copy of grass
   mapg[plr.z]
       [tar_x]
       [tar_y] = t_copy(
                  b_map[0])
   plr.tar = mapg[plr.z]
       [tar_x]
       [tar_y]
  end   
 end
end

function restore_block()
 local tar_x, tar_y =
  plr_tar_xy()
 	mapg[plr.z]
      [tar_x]
      [tar_y] = 
   t_copy(b_map[plr.tar.id])
end
